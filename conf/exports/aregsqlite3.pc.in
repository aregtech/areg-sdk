prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}/tools/areg
libdir=${prefix}/lib
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@

Name: aregsqlite
Description: This SQLite3 SQL database engine is compiled in AREG SDK project. If you want to compile AREG SDK projects with available sqlite3 package, re-compile AREG-SDK using available sqlite3 package.
Version: @PROJECT_VERSION@
URL: https://www.sqlite.org
Libs: -L${libdir}
Cflags: -I${includedir}
